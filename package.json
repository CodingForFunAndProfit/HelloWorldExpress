{
    "name": "helloworldexpress",
    "version": "1.0.0",
    "description": "",
    "main": "dist/index.js",
    "scripts": {
        "dev": "set DEBUG=helloworldexpress && nodemon --exec ts-node ./src/server.ts",
        "prebuild": "tslint -c tslint.json -p tsconfig.json --fix",
        "build": "tsc",
        "heroku-postbuild": "npm run build",
        "start": "npm run serve",
        "serve": "set NODE_ENV=production && node ./dist/server",
        "watch-node": "set DEBUG=helloworldexpress && node ./dist/server",
        "watch-ts": "tsc -w",
        "test": "jest --detectOpenHandles --forceExit test"
    },
    "engines": {
        "node": "11.13.0",
        "npm": "6.9.0"
    },
    "jest": {
        "transform": {
            "^.+\\.tsx?$": "ts-jest"
        },
        "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
        "moduleFileExtensions": [
            "ts",
            "tsx",
            "js",
            "jsx",
            "json",
            "node"
        ]
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/CodingForFunAndProfit/HelloWorldExpress.git"
    },
    "author": "",
    "license": "ISC",
    "bugs": {
        "url": "https://github.com/CodingForFunAndProfit/HelloWorldExpress/issues"
    },
    "homepage": "https://github.com/CodingForFunAndProfit/HelloWorldExpress#readme",
    "dependencies": {
        "apollo-server-express": "2.4.8",
        "bcryptjs": "2.4.3",
        "compression": "1.7.4",
        "connect-redis": "3.4.1",
        "cors": "2.8.5",
        "dotenv": "7.0.0",
        "express": "4.16.4",
        "express-session": "1.16.1",
        "graphql": "14.2.1",
        "graphql-query-complexity": "0.2.3",
        "ioredis": "4.9.0",
        "nodemailer": "6.1.0",
        "pg": "7.9.0",
        "pg-connection-string": "2.0.0",
        "reflect-metadata": "0.1.13",
        "type-graphql": "0.17.1",
        "typeorm": "0.2.16",
        "uuid": "3.3.2"
    },
    "devDependencies": {
        "@types/bcryptjs": "2.4.2",
        "@types/compression": "0.0.36",
        "@types/connect-redis": "0.0.9",
        "@types/cors": "2.8.4",
        "@types/debug": "4.1.4",
        "@types/dotenv": "6.1.1",
        "@types/express": "4.16.1",
        "@types/express-session": "1.15.12",
        "@types/faker": "4.1.5",
        "@types/graphql": "14.2.0",
        "@types/ioredis": "4.0.10",
        "@types/jest": "24.0.11",
        "@types/node": "11.13.4",
        "@types/nodemailer": "4.6.7",
        "@types/pg-connection-string": "2.0.0",
        "@types/supertest": "2.0.7",
        "@types/uuid": "3.4.4",
        "faker": "4.1.0",
        "jest": "24.7.1",
        "nodemon": "1.18.11",
        "supertest": "4.0.2",
        "ts-jest": "24.0.2",
        "ts-node": "8.0.3",
        "tslint": "5.15.0",
        "typescript": "3.4.3"
    }
}
